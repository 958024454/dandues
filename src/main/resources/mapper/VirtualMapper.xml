<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ht.dandues.Mapper.VirtualMapper">
    <select id="queryVirtualByUid"  resultType="Virtual">
        select * from `virtual` where uid=#{uid} and time='${time}'
    </select>

</mapper>
